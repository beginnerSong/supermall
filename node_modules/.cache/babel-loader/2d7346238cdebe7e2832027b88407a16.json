A,OAAA,EAAA;AACA,mBAAA,KAAA;AACA;;AACA,iBAAA,IAAA;AACA;AAlBA;AAAA;AAAA;AAAA;AAAA;AAoBA;AA/BA,GALA;AAsCA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,UAAA,KAAA,WAAA,EAAA;AAAA;AACA,aAAA,MAAA,CAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA;AAAA,iBAAA,IAAA,CAAA,OAAA,GAAA,KAAA;AAAA,SAAA;AACA,OAFA,MAEA;AAAA;AACA,aAAA,MAAA,CAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA;AAAA,iBAAA,IAAA,CAAA,OAAA,GAAA,IAAA;AAAA,SAAA;AACA;AACA,KAPA;AAQA,IAAA,SARA,uBAQA;AACA,UAAA,CAAA,KAAA,WAAA,EAAA;AACA,aAAA,MAAA,CAAA,IAAA,CAAA,SAAA,EAAA,IAAA;AACA;AACA;AAZA;AAtCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"cart-bottom-bar\">\r\n    <div class=\"check-content\">\r\n      <check-button :is-checked=\"isSelectAll\" class=\"check-botton\" @click.native=\"checkClick\"/>\r\n      <span>全选</span>\r\n    </div>\r\n\r\n    <div class=\"price\">\r\n      合计:{{totalPrice}}\r\n    </div>\r\n\r\n    <div class=\"calculate\" @click=\"calcClick\">\r\n      去计算({{checkLength}})\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n <script>\r\nimport CheckButton from 'components/content/checkButton/CheckButton'\r\n\r\n export default {\r\n   name: 'CartBottomBar',\r\n   components: {\r\n     CheckButton\r\n   },\r\n   computed: {\r\n     totalPrice() {\r\n       return '￥' + this.$store.state.cartList.filter(item => {\r\n         return item.checked\r\n       }).reduce((preValue, item) => {\r\n         return preValue + item.price * item.count\r\n       }, 0).toFixed(2)\r\n     },\r\n     checkLength() {\r\n       return this.$store.state.cartList.filter(item => item.checked).length\r\n     },\r\n     isSelectAll() {\r\n      //  如果没有数据 默认不选中\r\n      if(this.$store.state.cartList.length === 0) return false\r\n\r\n      // 选中状态判断 方案一: filter 过滤函数\r\n      //  输出的是一个false     item => !item.checked  没有被选中的        !(this.$store.state.cartList.filter(item => !item.checked).length)  没有被选中的长度取反 为false\r\n      // return !(this.$store.state.cartList.filter(item => !item.checked).length)  \r\n\r\n      // 选中状态判断 方案二: find 查找函数\r\n      // 找到没有被选中的之后取反\r\n      // return !this.$store.state.cartList.find(item => !item.checked)\r\n\r\n      // 方案三  普通的遍历函数\r\n      for (let item of this.$store.state.cartList) {\r\n        if (!item.checked) {\r\n          return false\r\n        }\r\n        return true\r\n      }\r\n\r\n     }\r\n   },\r\n   methods: {\r\n     checkClick() {\r\n       if (this.isSelectAll) { // this.isSelectAll之前的全选函数 即全部选中的情况\r\n         this.$store.state.cartList.forEach(item => item.checked = false);\r\n       } else {  // 部分或者全部未选中\r\n         this.$store.state.cartList.forEach(item => item.checked = true);\r\n       }\r\n     },\r\n     calcClick() {\r\n       if(!this.isSelectAll) {\r\n         this.$toast.show('请选择购买商品', 1500)\r\n       }\r\n     }\r\n   }\r\n }\r\n </script>\r\n\r\n <style scoped>\r\n  .cart-bottom-bar {\r\n    position: relative;\r\n    display: flex;\r\n\r\n    height: 40px;    \r\n    line-height: 40px;\r\n\r\n    background-color: #eee;\r\n  }\r\n  .check-content {\r\n    display: flex;\r\n    align-items: center;\r\n    margin-left: 10px;\r\n    width: 60px;\r\n  }\r\n\r\n  .check-botton {\r\n    width: 22px;\r\n    height: 22px;\r\n    line-height: 22px;\r\n    margin-right: 5px;\r\n  }\r\n\r\n  .price {\r\n    margin-left: 20px;\r\n    flex: 1;\r\n  }\r\n\r\n  .calculate {\r\n    width: 90px;\r\n    background-color: red;\r\n    color: #fff;\r\n    text-align: center;\r\n  }\r\n </style>"],"sourceRoot":"src/views/cart/childComps"}]}